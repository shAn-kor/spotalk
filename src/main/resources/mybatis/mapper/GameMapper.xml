<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
        
        
        
<mapper namespace="com.myweb.game.model.GameMapper">

 	<insert id="scrapScheduleKBO" parameterType="com.myweb.game.model.GameDTO">
		INSERT 
		INTO GAME(GAME_ID, GAME_DATE, TEAM_A, TEAM_B, GAME_RESULT) 
		VALUES (#{gameId}, #{gameDate}, #{teamA}, #{teamB}, #{gameResult}) 
	</insert> 
	
	<select id="getGame" resultType="com.myweb.game.model.GameDTO">
		SELECT * FROM GAME
	</select>
	
	
</mapper>